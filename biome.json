{
	"$schema": "https://biomejs.dev/schemas/2.2.4/schema.json",
	"formatter": {
		"enabled": true,
		"lineWidth": 120
	},
	"linter": {
		"enabled": true,
		"rules": {
			"nursery": {
				"useSortedClasses": {
					"level": "warn",
					"fix": "safe"
				}
			}
		}
	},
	"assist": {
		"enabled": true
	}
}
